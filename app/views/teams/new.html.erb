<div class="navbar-spacer"></div>
<div class="row">
  <div class="two columns">
  </div>
  <div class="ten columns">
    <%= form_for @team do |f| %>
      <div class="row">
          <label for=""> Name:</label> <%= f.text_field :name %>
      </div>
      <div class="row">
          <label for=""> Story of the team:</label> <%= f.text_area :description %>
      </div>

      <%= f.submit "Game on" %>
    <% end %>
  </div>
</div>
